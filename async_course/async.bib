@article{jewell2012,
    author = "Jewell, C. P. and Roberts, G. O.",
    title = "Enhancing {{Bayesian}} Risk Prediction for Epidemics Using Contact Tracing",
    year = "2012",
    month = "September",
    journal = "Biostatistics",
    volume = "13",
