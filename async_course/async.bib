@article{fields2018,
    author = "Fields, Deborah Ann and Kafai, Yasmin and Nakajima, Tomoko and Goode, Joanna and Margolis, Jane",
    title = "Putting {{Making}} into {{High School Computer Science Classrooms}}: {{Promoting Equity}} in {{Teaching}} and {{Learning}} with {{Electronic Textiles}} in {{{\emph{Exploring Computer Science}}}}",
    shorttitle = "Putting {{Making}} into {{High School Computer Science Classrooms}}",
