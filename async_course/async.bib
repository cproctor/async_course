@article{milner2020,
    author = "Milner, H. Richard",
    title = "Fifteenth {{Annual AERA Brown Lecture}} in {{Education Research}}: {{Disrupting Punitive Practices}} and {{Policies}}: {{Rac}}(e)Ing {{Back}} to {{Teaching}}, {{Teacher Preparation}}, and {{Brown}}",
    shorttitle = "Fifteenth {{Annual AERA}} {{{\emph{Brown}}}} {{Lecture}} in {{Education Research}}",
    year = "2020",
    month = "April",
    journal = "Educational Researcher",
    volume = "49",
    number = "3",
