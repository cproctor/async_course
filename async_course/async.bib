@inproceedings{hajian2011,
    author = "Hajian, Sara and {Domingo-Ferrer}, Josep and {Martinez-Balleste}, Antoni",
    title = "Discrimination Prevention in Data Mining for Intrusion and Crime Detection",
    booktitle = "2011 {{IEEE Symposium}} on {{Computational Intelligence}} in {{Cyber Security}} ({{CICS}})",
    year = "2011",
    month = "April",
    pages = "47--54",
    publisher = "{IEEE}",
    address = "{Paris, France}",
