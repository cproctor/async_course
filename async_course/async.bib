@article{schneider2016a,
    author = "Schneider, Bertrand and Sharma, Kshitij and Cuendet, S{\'e}bastien and Zufferey, Guillaume and Dillenbourg, Pierre and Pea, Roy",
    title = "Using {{Mobile Eye-Trackers}} to {{Unpack}} the {{Perceptual Benefits}} of a {{Tangible User Interface}} for {{Collaborative Learning}}",
    year = "2016",
