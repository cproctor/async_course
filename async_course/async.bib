@misc{levine2019,
    author = "Levine, Sarah",
    title = "Representing {{Knowledge}}, {{Thinking}}, and {{Expertise}}",
    year = "2019",
    abstract = "This course offers an overview of ways that the fields of learning sciences and psychology have characterized knowledge, learning, and expertise. We will look at more general models of knowledge representation (e.g. as a set of scripts and schemata, as socially mediated, as embodied), knowledge representation in specific domains (e.g. playing chess, solving math problems, waiting tables, or constructing literary interpretations), and knowledge representation in teaching. In the first part of the course, we will examine seminal studies of knowledge representation and expert/novice practice, attempt to apply some of those models to our own thinking and research interests, and take a critical look at the notion of expertise as it relates to race, class, gender, and power. During the second part of the course, you will build your own comparative study of expert and novice thinking in a domain that interests you."
}

@article{levy2010,
    author = "Levy, S. T. and Mioduser, D.",
    title = "Approaching {{Complexity Through Planful Play}}: {{Kindergarten Children}}'s {{Strategies}} in {{Constructing}} an {{Autonomous Robot}}'s {{Behavior}}",
    shorttitle = "Approaching {{Complexity Through Planful Play}}",
    year = "2010",
    month = "April",
    journal = "International Journal of Computers for Mathematical Learning",
    volume = "15",
    number = "1",
    pages = "21--43",
    issn = "1382-3892, 1573-1766",
    doi = "10.1007/s10758-010-9159-5",
    abstract = "This study investigates how young children master, construct and understand intelligent rule-based robot behaviors, focusing on their strategies in gradually meeting the tasks' complexity. The wider aim is to provide a comprehensive map of the kinds of transitions and learning that take place in constructing simple emergent behaviors, particularly for young children. Six kindergarten children participated individually in the study along five sessions. Regarding modes of engagement, it was found that the children conducted intensive and extended playful investigations of the robot's behaviors, interacting with it in a variety of ways; it was also found that their constructions were planful and anticipatory, as they could simulate how the behaviors play out even prior to running their programs. Three kinds of transitions were found in the children's comprehension of the system: one involved adaptation to the formal language; the second, coordination of multiple spatial perspectives; and the third involved a shift from viewing rules as one-time events to their view as recurring and continual descriptions of a process. Finally, it was found that the children employed two strategies to reduce the amount of information in the system: ``pruning'' involved ignoring part of the logical structure and focusing on another; ``fusing'' involved coalescing several rules or functions into one. These results are discussed with respect to previous literature on children's programming and with regards to understanding and supporting young children's learning through their construction of adaptive autonomous behaviors.",
    langid = "english"
}

@inproceedings{lewis2019,
    author = "Lewis, Colleen and Bruno, Paul and Raygoza, Jonathan and Wang, Julia",
    title = "Alignment of {{Goals}} and {{Perceptions}} of {{Computing Predicts Students}}' {{Sense}} of {{Belonging}} in {{Computing}}",
    booktitle = "Proceedings of the 2019 {{ACM Conference}} on {{International Computing Education Research}}",
    year = "2019",
    month = "July",
    pages = "11--19",
    publisher = "{ACM}",
    address = "{Toronto ON Canada}",
    doi = "10.1145/3291279.3339426",
    abstract = "The lack of diversity has been a troubling issue for the field of computing. As the industry continues to expand, it is imperative that diversity levels in computing fields increase to fill the ever-growing number of computing jobs and to represent the technological needs of a diverse populace. Our research seeks to better understand the factors that lead to this lack of diversity by analyzing data from computing majors from colleges within the United States and Canada. Our study reveals that students of different demographic backgrounds have varying levels of communal values, which, depending on how they perceive the affordance of computing to fulfill such values, predict their sense of belonging in computing, which is an important predictor of student retention.",
    isbn = "978-1-4503-6185-9",
